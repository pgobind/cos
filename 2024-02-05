 Dataset<Row> updatedDataFrame = spark.sql(
                "SELECT p.id_system, " +
                "       COALESCE(p2.id_gmi_cust, p.id_gmi_cust) AS id_gmi_cust, " +
                "       p.id_group " +
                "FROM ptrf_gmi_ac p " +
                "LEFT JOIN distinctDataFrame p2 " +
                "ON p.id_system = p2.id_system AND p.id_group = p2.id_group"
        );
